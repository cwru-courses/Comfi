{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "react/jsx-filename-extension": 0,
        "react/prop-types": ["off"],
        "global-require": "off",
        "react/jsx-no-constructed-context-values": "off",
        "react/no-unstable-nested-components": "off"
    }
}
